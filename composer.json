{
  "name": "level23/druid-client",
  "description": "Druid client for executing queries.",
  "keywords": [
    "level23"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "illuminate/support": "~5.0",
    "mockery/mockery": "^1.2",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/phpstan": "^0.11.9",
    "phpstan/phpstan-mockery": "^0.11.2",
    "phpunit/phpunit": "^8.2"
  },
  "suggest": {
    "psr/log": "Required for using the log handler"
  },
  "autoload": {
    "psr-4": {
      "Level23\\Druid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Level23\\Druid\\DruidServiceProvider"
      ],
      "aliases": {
        "Druid": "Level23\\Druid\\Facades\\Druid"
      }
    }
  }
}
