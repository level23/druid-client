parameters:
    level: max
    includes:
      - vendor/timeweb/phpstan-enum/extension.neon
      - vendor/phpstan/phpstan-mockery/extension.neon
    ignoreErrors:
      - '#Call to an undefined method (.*)?Mockery\\(.*)::(.*)#'
    excludes_analyse:
      - src/DruidServiceProvider.php
    paths:
      - tests
      - src